<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context" xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca-j/spring/200810"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
            http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
            http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
            http://docs.oasis-open.org/ns/opencsa/sca-j/spring/200810 http://www.springframework.org/schema/sca http://www.springframework.org/schema/sca/spring-sca.xsd">

    <context:component-scan base-package="com.lenovo.vctl.apps"/>
    <bean id="dao" class="com.lenovo.vctl.dal.dao.impl.CompositeDaoImpl"/>

    <sca:reference name="vctlUserService" type="com.lenovo.vctl.apps.user.service.VctlUserService"></sca:reference>
    <sca:reference name="banWordService" type="com.lenovo.vctl.apps.wordban.service.BanWordService"></sca:reference>
    <sca:reference name="configService" type="com.lenovo.vctl.apps.config.service.ConfigService"></sca:reference>
    <sca:reference name="unreadRemoteService"
                   type="com.lenovo.vctl.apps.pushserver.service.remote.UnreadRemoteService"></sca:reference>
    <sca:reference name="userAutoAnswerRemoteService" type="com.lenovo.vctl.apps.contacts.service.remote.UserAutoAnswerRemoteService"></sca:reference>
	<sca:reference name="blackListRemoteService" type="com.lenovo.vctl.apps.blacklist.service.remote.BlackListRemoteService"></sca:reference>
    <sca:reference name="userCandidateRemoteService" type="com.lenovo.vctl.apps.recommend.service.remote.UserCandidateRemoteService"></sca:reference>
    
    <sca:service name="UserRelationRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserRelationRemoteService"
                 target="UserRelationRemoteServiceBean"></sca:service>
    <bean id="UserRelationRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserRelationRemoteServiceImpl">
        <property name="banWordService" ref="banWordService"></property>
    </bean>

    <sca:service name="UserRecordsRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserRecordsRemoteService"
                 target="UserRecordsRemoteServiceBean"></sca:service>
    <bean id="UserRecordsRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserRecordsRemoteServiceImpl">
    </bean>

    <sca:service name="UserRecordsBackupRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserRecordsBackupRemoteService"
                 target="UserRecordsBackupRemoteServiceBean"></sca:service>
    <bean id="UserRecordsBackupRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserRecordsBackupRemoteServiceImpl">
    </bean>

    <sca:service name="UserUpdateInfoRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserUpdateInfoRemoteService"
                 target="UserUpdateInfoRemoteServiceBean"></sca:service>
    <bean id="UserUpdateInfoRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserUpdateInfoRemoteServiceImpl">
    </bean>

    <sca:service name="NoticeRemoteService" type="com.lenovo.vctl.apps.contacts.service.remote.NoticeRemoteService"
                 target="NoticeRemoteServiceBean"></sca:service>
    <bean id="NoticeRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.NoticeRemoteServiceImpl">
    </bean>
    <sca:service name="InviteRecordRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.InviteRecordRemoteService"
                 target="InviteRecordRemoteServiceBean"></sca:service>
    <bean id="InviteRecordRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.InviteRecordRemoteServiceImpl">
    </bean>

    <sca:service name="FrequentContactRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.FrequentContactRemoteService"
                 target="FrequentContactRemoteServiceBean"></sca:service>
    <bean id="FrequentContactRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.FrequentContactRemoteServiceImpl">
    </bean>

    <sca:service name="UserHistoryRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserHistoryRemoteService"
                 target="UserHistoryRemoteServiceBean">
    </sca:service>
    <bean id="UserHistoryRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserHistoryRemoteServiceImpl">
    </bean>


    <sca:service name="UserConvHistoryRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserConvHistoryRemoteService"
                 target="UserConvHistoryRemoteServiceBean">
    </sca:service>
    <bean id="UserConvHistoryRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserConvHistoryRemoteServiceImpl">
    </bean>

    <sca:service name="UserRecommendRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserRecommendRemoteService"
                 target="UserRecommendRemoteServiceBean">
    </sca:service>
    <bean id="UserRecommendRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserRecommendRemoteServiceImpl">
    </bean>
    <sca:service name="userCommunicationRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserCommunicationRemoteService"
                 target="userCommunicationRemoteServiceBean">
    </sca:service>
    <bean id="userCommunicationRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserCommunicationRemoteServiceImpl">
    </bean>

    <sca:service name="InvitationService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.InvitationService"
                 target="InvitationServiceBean">
    </sca:service>
    <bean id="InvitationServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.InvitationServiceImpl">
    </bean>

    <sca:service name="UserInfoStrategy"
                 type="com.lenovo.vctl.apps.contacts.service.strategy.UserInfoStrategy"
                 target="UserInfoStrategyBean">
    </sca:service>
    <bean id="UserInfoStrategyBean"
          class="com.lenovo.vctl.apps.contacts.service.strategy.impl.UserInfoStrategyImpl">
    </bean>

    <sca:service name="UserInfoQueueStrategy"
                 type="com.lenovo.vctl.apps.contacts.service.strategy.UserInfoStrategy"
                 target="UserInfoQueueStrategyBean">
    </sca:service>
    <bean id="UserInfoQueueStrategyBean"
          class="com.lenovo.vctl.apps.contacts.service.strategy.impl.UserInfoQueueStrategyImpl">
    </bean>
    <sca:service name="UserTextRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserTextRemoteService"
                 target="UserTextRemoteServiceBean">
    </sca:service>
    <bean id="UserTextRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserTextRemoteServiceImpl">
    </bean>
    <sca:service name="VctlTextMsgRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.VctlTextMsgRemoteService"
                 target="VctlTextMsgRemoteServiceBean">
    </sca:service>
    <bean id="VctlTextMsgRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.VctlTextMsgRemoteServiceImpl">
    </bean>
    <sca:service name="UserAutoAnswerRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserAutoAnswerRemoteService"
                 target="UserAutoAnswerRemoteServiceBean">
    </sca:service>
    <bean id="UserAutoAnswerRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserAutoAnswerRemoteServiceImpl">
    </bean>
    <sca:service name="RandomCallRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.RandomCallRemoteService"
                 target="RandomCallRemoteServiceBean">
    </sca:service>
    <bean id="RandomCallRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.RandomCallRemoteServiceImpl">
    </bean>
    <sca:service name="userRelationDeleteRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserRelationDeleteRemoteService"
                 target="userRelationDeleteRemoteServiceBean">
    </sca:service>
    <bean id="userRelationDeleteRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserRelationDeleteRemoteServiceImpl">
    </bean>
    <sca:service name="userMayKnowRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserMayKnowRemoteService"
                 target="userMayKnowRemoteServiceBean">
    </sca:service>
    <bean id="userMayKnowRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserMayKnowRemoteServiceImpl">
    </bean>
    <!-- 陌生人打招呼的列表-->
    <sca:service name="UserHelloRemoteService"
                 type="com.lenovo.vctl.apps.contacts.service.remote.UserHelloRemoteService"
                 target="UserHelloRemoteServiceBean">
    </sca:service>
    <bean id="UserHelloRemoteServiceBean"
          class="com.lenovo.vctl.apps.contacts.service.remote.impl.UserHelloRemoteServiceImpl">
    </bean>


    <bean id="dbConfiguration" class="com.lenovo.vctl.apps.config.DBConfiguration">
        <property name="configKeys"
                  value="MAX_USER_RELATION,MAX_USER_RELATION_FAKE,MAX_FREQUENT_USER_RELATION,MAX_COMMON_USER_RELATION,LIMIT_EMAIL,LIMIT_SHOTMSG,LIMIT_INVITE_TIMESTAMP,DEFAULT_USER_MOBILE"/>
    </bean>


</beans>
  
